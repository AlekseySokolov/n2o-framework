<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-4.0">
    <datasources>
        <datasource id="ds"/>
        <datasource id="ds2"/>
        <inherited-datasource id="ds3" source-datasource="ds"/>
        <app-datasource id="appDs"/>
    </datasources>

    <toolbar>
        <button>
            <show-modal page-id="testParentDatasourceModal" route="/modal1"
                        submit-action-type="copy"
                        copy-datasource="ds2"
                        target-datasource="ds3">
            </show-modal>
        </button>
        <button>
            <show-modal page-id="testParentDatasourceModal" route="/modal2"
                        submit-action-type="copy"
                        copy-datasource="ds3"
                        target-datasource="ds2">
            </show-modal>
        </button>
    </toolbar>
</page>
