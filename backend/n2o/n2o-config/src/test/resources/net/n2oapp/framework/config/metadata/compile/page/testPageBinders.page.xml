<page xmlns="http://n2oapp.net/framework/config/schema/page-3.0" name="Hello, {name} {code}" show-title="true">
    <regions>
        <region place="test">
            <form id="main">
                <fields>
                    <input-text id="name" default-value="#{test}"/>
                    <select id="gender">
                        <default-value id="#{test}"/>
                        <options>
                            <option>{"id":1}</option>
                            <option>{"id":2}</option>
                        </options>
                    </select>
                    <date-interval id="birthday">
                        <default-value end="11.11.2018" begin="01.11.2018"/>
                    </date-interval>
                    <interval-field id="intervalTest">
                        <begin>
                            <input-text id="beginTest"/>
                        </begin>
                        <end>
                            <input-text id="endTest" default-value="156"/>
                        </end>
                    </interval-field>
                    <input-text id="type" param="param_type" default-value="55"/>
                </fields>
            </form>
        </region>
    </regions>
</page>