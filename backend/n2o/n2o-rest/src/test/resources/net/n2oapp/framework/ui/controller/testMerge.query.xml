<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <list>
        <test operation="findAll" file="net/n2oapp/framework/ui/controller/testDefaults.json"/>
    </list>

    <fields>
        <field id="id">
            <select>id</select>
            <filters>
                <eq filter-id="id">id:eq:id</eq>
            </filters>
        </field>
        <field id="name">
            <select>name</select>
        </field>
        <field id="surname">
            <select>surname</select>
        </field>
    </fields>
</query>
