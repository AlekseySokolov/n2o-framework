<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">

    <unique>
        <test file="net/n2oapp/framework/engine/normalize/testListUtilsUnion.json" operation="findOne"/>
    </unique>

    <fields>
        <list id="users" normalize="#union(#this['teachers'], #this['students'])">
            <field id="name"/>
        </list>
    </fields>
</query>
