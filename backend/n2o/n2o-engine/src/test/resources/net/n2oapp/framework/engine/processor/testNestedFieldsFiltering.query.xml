<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    
    <list>
        <test file="net/n2oapp/framework/engine/processor/testNestedFieldsFiltering.json"/>
    </list>
    
    <filters>
        <eq field-id="id"/>
        <eq field-id="organization.code"/>
    </filters>
    
    <fields>
        <field id="id"/>
        <field id="name"/>
        <reference id="organization">
            <field id="code"/>
            <field id="title"/>
            <list id="employees">
                <field id="id"/>
                <field id="name"/>
            </list>
        </reference>
    </fields>
</query>
