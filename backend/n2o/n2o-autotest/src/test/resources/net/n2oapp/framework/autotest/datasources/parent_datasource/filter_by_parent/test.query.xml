<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0"
       object-id="test">
    <list>
        <test file="net/n2oapp/framework/autotest/datasources/parent_datasource/filter_by_parent/test.json" operation="findAll"/>
    </list>

    <filters>
        <eq field-id="id" filter-id="id"/>
        <in field-id="id" filter-id="ids"/>
    </filters>

    <fields>
        <field id="id" domain="integer"/>
        <field id="name"/>
        <list id="persons">
            <field id="value"/>
        </list>
    </fields>
</query>
