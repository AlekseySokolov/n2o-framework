<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="net/n2oapp/framework/autotest/multi_fieldset/filtering/test.json"/>
    </list>

    <filters>
        <eq field-id="id" filter-id="id"/>
        <eq field-id="name" filter-id="name"/>
        <eq field-id="type" filter-id="type"/>
        <eq field-id="chapter" filter-id="chapter"/>
    </filters>

    <fields>
        <field id="id"/>
        <field id="name"/>
        <field id="type"/>
        <field id="chapter"/>
    </fields>
</query>