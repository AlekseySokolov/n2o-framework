<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">

    <list>
        <java method="sendNotFound" class="net.n2oapp.framework.autotest.run.ErrorController">
            <spring/>
        </java>
    </list>

    <unique>
        <java method="sendNotFound" class="net.n2oapp.framework.autotest.run.ErrorController">
            <spring/>
        </java>
    </unique>

    <count>
        <java method="sendNotFound" class="net.n2oapp.framework.autotest.run.ErrorController">
            <spring/>
        </java>
    </count>

    <fields>
        <field id="id">
            <select/>
        </field>

        <field id="firstName">
            <select/>
        </field>

        <field id="lastName">
            <select/>
        </field>

    </fields>
</query>

