<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-5.0">
    <list>
        <test file="test.json"/>
    </list>
    <count>
        <test file="test.json" operation="count"/>
    </count>

    <filters>
        <eq field-id="id" filter-id="id"/>
        <eq field-id="type" filter-id="type"/>
        <eq field-id="name" filter-id="name_eq"/>
        <like field-id="name" filter-id="name"/>
    </filters>

    <fields>
        <field id="id" domain="integer"/>
        <field id="type"/>
        <field id="name"/>
    </fields>
</query>
