<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-4.0"
             name="Страница для автотеста по проверки выбора элемента dropdown через enter">
    <form >
        <fields >
            <row>
                <input-select
                        placeholder="Введите значение"
                        id="inputSelectWithStatus"
                        label="Ввод с выпадающим списком"
                        label-field-id="code"
                        query-id="data"
                >
                </input-select>
            </row>
        </fields>
        <toolbar place="topLeft">
            <button label="unselect" enabled="true"/>
        </toolbar>
    </form>

</simple-page>